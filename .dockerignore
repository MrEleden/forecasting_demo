data/processed/*
!data/processed/.gitkeep

# Models
models/artifacts/*
!models/artifacts/.gitkeep
checkpoints/*
!checkpoints/.gitkeep
*.pth
*.ckpt
*.safetensors

# Logs and outputs
*.log
logs/
outputs/
mlruns/
results/
htmlcov/
.coverage
.pytest_cache/
.mypy_cache/
.ruff_cache/

# IDEs
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Git
.git/
.gitignore
.gitattributes

# Documentation
docs/_build/
*.md
!README.md

# CI/CD
.github/
.pre-commit-config.yaml

# Development
*.ipynb
.jupyter/
notebooks/

